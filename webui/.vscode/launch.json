{

    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "vuejs: chrome",
        "url": "http://localhost:5173",
        "webRoot": "${workspaceFolder}",
        "sourceMapPathOverrides": {
           "webpack:///./*": "${webRoot}/*"
        },
        "sourceMaps": true
      }
    ]
}